<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Activity.MenuListActivity">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/black"
                android:fontFamily="@font/gmarketsans"
                android:gravity="center"
                android:minHeight="60dp"
                android:password="false"
                android:text="app name"
                android:textColor="@color/white"
                android:textSize="40sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="30dp"
                android:layout_marginStart="7dp"
                android:layout_marginTop="35dp"
                android:background="@color/black"
                android:fontFamily="@font/gmarketsans"
                android:orientation="vertical"
                android:text=" 오늘의 운동 "
                android:textColor="@color/white"
                android:textSize="22sp"
                android:textStyle="bold" />


            <!--<com.github.mikephil.charting.charts.BarChart
                    android:id="@+id/chart"
                    android:layout_width="match_parent"
                    android:layout_height="300dp"
                    android:layout_margin="10dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />
        -->

            <HorizontalScrollView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="horizontal">

                    <androidx.cardview.widget.CardView
                        android:layout_width="175dp"
                        android:layout_height="95dp"
                        android:layout_margin="5dp"
                        app:cardElevation="5dp">

                        <com.pierfrancescosoffritti.androidyoutubeplayer.core.player.views.YouTubePlayerView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:scaleType="fitXY"
                            app:autoPlay="false"
                            app:videoId="5IBhz4c2D4o" />

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="175dp"
                        android:layout_height="95dp"
                        android:layout_margin="5dp"
                        app:cardElevation="5dp">

                        <com.pierfrancescosoffritti.androidyoutubeplayer.core.player.views.YouTubePlayerView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:autoPlay="false"
                            app:videoId="_RXjbRdiFBs" />

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="195dp"
                        android:layout_height="95dp"
                        android:layout_margin="5dp"
                        app:cardElevation="5dp">

                        <com.pierfrancescosoffritti.androidyoutubeplayer.core.player.views.YouTubePlayerView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:autoPlay="false"
                            app:videoId="g6enC_9eO24" />

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="195dp"
                        android:layout_height="95dp"
                        android:layout_margin="5dp"
                        app:cardElevation="5dp">

                        <com.pierfrancescosoffritti.androidyoutubeplayer.core.player.views.YouTubePlayerView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            app:autoPlay="false"
                            app:videoId="DZAqP67WQNU" />

                    </androidx.cardview.widget.CardView>


                </LinearLayout>

            </HorizontalScrollView>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <CalendarView
                    android:id="@+id/Calendar"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="39dp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="bottom"
                    android:layout_weight="1"
                    android:orientation="horizontal">

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/btn_home"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:layout_weight="1"
                        android:adjustViewBounds="true"
                        android:background="@color/white"
                        android:drawableTop="@drawable/ic_baseline_home_24"
                        android:scaleType="fitStart"
                        android:text="Home"
                        android:textSize="11sp" />

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/btn_community"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:layout_weight="1"
                        android:background="@color/white"
                        android:drawableTop="@drawable/ic_baseline_person_24"
                        android:text="Community"
                        android:textSize="11sp" />

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/btn_setting"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:layout_weight="1"
                        android:background="@color/white"
                        android:drawableTop="@drawable/ic_baseline_settings_24"
                        android:text="Setting"
                        android:textSize="11sp" />

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/btn_menu"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="bottom"
                        android:layout_weight="1"
                        android:background="@color/material_dynamic_neutral80"
                        android:drawableTop="@drawable/ic_baseline_dehaze_24"
                        android:text="menu"
                        android:textSize="11sp" />

                </LinearLayout>

            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="150dp"
            android:layout_marginBottom="42dp"
            android:gravity="bottom|end"
            android:orientation="vertical">

            <Button
                android:id="@+id/btn_go_dictionary"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/material_dynamic_neutral80"
                android:fontFamily="@font/gmarketsans"
                android:insetTop="0dp"
                android:insetBottom="0dp"
                android:text="칼로리 사전"
                android:textColor="@color/white"
                android:textSize="16sp"
                android:textStyle="bold"
                app:strokeColor="@color/material_dynamic_neutral60"
                app:strokeWidth="1dp" />

            <Button
                android:id="@+id/btn_go_bmi_and_cal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/material_dynamic_neutral80"
                android:fontFamily="@font/gmarketsans"
                android:insetTop="0dp"
                android:insetBottom="0dp"
                android:text="BMI 계산기 /  칼로리 처방"
                android:textSize="16sp"
                android:textStyle="bold"
                app:strokeColor="@color/material_dynamic_neutral60"
                app:strokeWidth="1dp" />

            <Button
                android:id="@+id/btn_go_diary"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/material_dynamic_neutral80"
                android:fontFamily="@font/gmarketsans"
                android:insetTop="0dp"
                android:insetBottom="0dp"
                android:text="다이어리"
                android:textSize="16sp"
                android:textStyle="bold"
                app:strokeColor="@color/material_dynamic_neutral60"
                app:strokeWidth="1dp" />

            <Button
                android:id="@+id/btn_go_alarm"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/material_dynamic_neutral80"
                android:fontFamily="@font/gmarketsans"
                android:insetTop="0dp"
                android:insetBottom="0dp"
                android:text="알람"
                android:textSize="16sp"
                android:textStyle="bold"
                app:strokeColor="@color/material_dynamic_neutral60"
                app:strokeWidth="1dp" />

            <Button
                android:id="@+id/btn_go_report"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/material_dynamic_neutral80"
                android:fontFamily="@font/gmarketsans"
                android:insetTop="0dp"
                android:insetBottom="0dp"
                android:text="월별 상세 리포트"
                android:textSize="16sp"
                android:textStyle="bold"
                app:strokeColor="@color/material_dynamic_neutral60"
                app:strokeWidth="1dp" />

            <Button
                android:id="@+id/btn_go_video"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/material_dynamic_neutral80"
                android:fontFamily="@font/gmarketsans"
                android:insetTop="0dp"
                android:insetBottom="0dp"
                android:isScrollContainer="true"
                android:text="운동 영상"
                android:textSize="16sp"
                android:textStyle="bold"
                app:strokeColor="@color/material_dynamic_neutral60"
                app:strokeWidth="1dp" />
        </LinearLayout>
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>